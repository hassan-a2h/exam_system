
<div class="card m-2">
  <h5 class="card-header">FITB</h5>
  <div class="card-body">
    <%= f.label :before_part %>
    <%= f.text_field :text_before %> <br>

    <%= f.label :answer %>
    <%= f.text_field :answer %> <br>

    <%= f.label :after_part %>
    <%= f.text_field :text_after %> <br>

    <%= f.label :marks %>
    <%= f.number_field :marks, in: 1..10 %> <br>
  </div>
</div>
